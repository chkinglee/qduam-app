<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>
        <variable
            name="dict"
            type="com.lilinzhen.qduamapplication.Utils.AcquireMenuActivity.Dict"/>
    </data>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/fifty_four"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="@dimen/zero"
                android:layout_height="wrap_content"
                android:layout_weight="@integer/one"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/twenty_four"
                android:layout_marginRight="@dimen/ten"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@{dict.label}"
                    android:textSize="@dimen/sixteen"
                    android:textColor="@color/project_title_background"/>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</layout>
